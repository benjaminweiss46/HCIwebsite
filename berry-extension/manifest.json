{
	"name": "Berry",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["https://meet.google.com/*"],
			"js": ["content.js"]//loads the js content of the page
		}
	],
	"browser_action": {
		"default_popup": "popup.html", 
		"default_title": "Berry",
		"default_icon": "berry533x580.jpg"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"tabs",
		"https://meet.google.com/*",
		"storage"
	]
}